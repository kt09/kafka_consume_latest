{
    "scenarioName": "Produce a message to kafka topic - int Key",
    "steps": [
        {
            "name": "load_kafka",
            "url": "kafka-topic:demo",
            "operation": "produce",
            "request": {
                "records":[
                    {
                        "key": 101,
                        "value": "Hello World"
                    }
                ]
            },
            "assertions": {
                "status" : "Ok",
                "recordMetadata" : "$NOT.NULL"
            }
        }
    ]
}
